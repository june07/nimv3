{
  "name": "nimv3",
  "version": "3.0.0",
  "scripts": {
    "dev": "vite --host",
    "dev:extension": "npm-run-all --parallel build:dev watch:rollup:deps",
    "build:dev": "vite build -w --base /dist/ --sourcemap --minify false",
    "build": "npm-run-all --serial build:prod build:deps",
    "build:prod": "vite build --base /dist/ --sourcemap --minify true",
    "build:deps": "NODE_ENV=production node utils/watch-rollup.js",
    "watch:rollup:deps": "node utils/watch-rollup.js",
    "preview": "vite preview",
    "test": "playwright test --project \"edge@latest@localhost\" \"chrome@latest@localhost\" --retries 3"
  },
  "dependencies": {
    "@auth0/auth0-vue": "^1.0.2",
    "@mdi/font": "7.0.96",
    "@vueuse/core": "^9.8.2",
    "amplitude-js": "^8.21.3",
    "animejs": "^3.2.1",
    "async": "^3.2.4",
    "jwt-decode": "^3.1.2",
    "pubnub": "^7.2.2",
    "socket.io-client": "^4.5.4",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "uuid": "^9.0.0",
    "vue": "^3.2.38",
    "vue-social-sharing": "^4.0.0-alpha4",
    "vuetify": "^3.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "browserstack-local": "^1.5.1",
    "chokidar": "^3.5.3",
    "eslint": "^8.31.0",
    "nanoid": "^3.3.4",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.29.1",
    "sass": "^1.56.2",
    "vite": "^4.0.2",
    "vite-plugin-vuetify": "^1.0.0-alpha.12",
    "vue-eslint-parser": "^9.1.0"
  }
}
