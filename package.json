{
    "name": "nimv3",
    "version": "3.13.2",
    "scripts": {
        "dev": "vite --host",
        "dev:extension": "npm-run-all --parallel build:dev watch:rollup:deps",
        "build:dev": "vite build --mode development -w --base /dist/ --sourcemap --minify false",
        "build": "npm-run-all --serial util:updateManifest build:prod build:deps",
        "build:prod": "vite build --base /dist/ --minify true",
        "build:deps": "cross-env NODE_ENV=production node utils/watch-rollup.js",
        "util:updateManifest": "cross-env NODE_ENV=production node utils/updateManifest.js",
        "watch:rollup:deps": "node utils/watch-rollup.js",
        "preview": "vite preview",
        "test": "playwright test --project \"edge@latest@localhost\" --workers 5 --retries 3",
        "test:local": "npm-run-all --serial build test:singleWorker",
        "test:singleWorker": "playwright test --project \"edge@latest@localhost\" --workers 1",
        "test:post": "playwright test --project \"edge@latest@localhost\" --workers 5 --retries 3"
    },
    "dependencies": {
        "@auth0/auth0-vue": "^2.4.0",
        "@vitejs/plugin-vue": "^5.2.1",
        "@vueuse/core": "^12.5.0",
        "animejs": "^3.2.2",
        "async": "^3.2.6",
        "jwt-decode": "^4.0.0",
        "nanoid": "^5.0.9",
        "posthog-js": "^1.215.1",
        "socket.io-client": "^4.8.1",
        "tweetnacl": "^1.0.3",
        "tweetnacl-util": "^0.15.1",
        "uuid": "^11.0.5",
        "vite": "^6.0.11",
        "vue": "^3.5.13",
        "vue-social-sharing": "^4.0.0-alpha4",
        "vuetify": "^3.7.9"
    },
    "devDependencies": {
        "@playwright/test": "^1.50.1",
        "browserstack-local": "^1.5.6",
        "chokidar": "^4.0.3",
        "cross-env": "^7.0.3",
        "eslint": "^9.19.0",
        "ndjson": "^2.0.0",
        "npm-run-all": "^4.1.5",
        "playwright": "^1.50.1",
        "sass": "^1.83.4",
        "vite-plugin-vuetify": "^2.0.4",
        "vue-eslint-parser": "^9.4.3"
    }
}