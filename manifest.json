{
  "manifest_version": 3,
  "name": "__MSG_appName__",
  "version": "3.6.11",
  "default_locale": "en",
  "description": "__MSG_appDescription__",
  "author": "June07 <667@june07.com> (https://june07.com/)",
  "commands": {
    "open-devtools": {
      "suggested_key": {
        "default": "Alt+I"
      },
      "description": "__MSG_keyCtrlI__"
    },
    "fifo-copy-paste": {
      "suggested_key": {
        "default": "Alt+C"
      },
      "description": "Fifo copy and paste"
    }
  },
  "action": {
    "default_icon": {
      "16": "/dist/icon/icon16.png",
      "32": "/dist/icon/icon32.png",
      "48": "/dist/icon/icon48.png",
      "128": "/dist/icon/icon128.png",
      "162": "/dist/icon/icon16@2x.png",
      "163": "/dist/icon/icon16@3x.png",
      "322": "/dist/icon/icon32@2x.png",
      "323": "/dist/icon/icon32@3x.png",
      "482": "/dist/icon/icon48@2x.png",
      "483": "/dist/icon/icon48@3x.png",
      "1282": "/dist/icon/icon128@2x.png",
      "1283": "/dist/icon/icon128@3x.png"
    },
    "default_title": "__MSG_appName__",
    "default_popup": "/dist/index.html"
  },
  "icons": {
    "16": "/dist/icon/icon16.png",
    "32": "/dist/icon/icon32.png",
    "48": "/dist/icon/icon48.png",
    "128": "/dist/icon/icon128.png",
    "162": "/dist/icon/icon16@2x.png",
    "163": "/dist/icon/icon16@3x.png",
    "322": "/dist/icon/icon32@2x.png",
    "323": "/dist/icon/icon32@3x.png",
    "482": "/dist/icon/icon48@2x.png",
    "483": "/dist/icon/icon48@3x.png",
    "1282": "/dist/icon/icon128@2x.png",
    "1283": "/dist/icon/icon128@3x.png"
  },
  "background": {
    "service_worker": "/src/sw.js"
  },
  "permissions": [
    "storage",
    "background",
    "tabs",
    "tabGroups",
    "identity",
    "identity.email",
    "notifications",
    "contextMenus",
    "debugger",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.june07.com/*",
      "*://*.brakecode.com/*"
    ]
  },
  "short_name": "__MSG_appShortName__",
  "version_name": "3.6.11"
}
